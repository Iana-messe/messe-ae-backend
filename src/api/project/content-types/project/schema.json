{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "constructionType": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "single-level",
        "double-decker"
      ]
    },
    "eventType": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "exhibition",
        "event"
      ]
    },
    "eventName": {
      "type": "string",
      "required": true
    },
    "eventDate": {
      "type": "date",
      "required": true
    },
    "firstFloorSize": {
      "type": "integer",
      "required": true
    },
    "secondFloorSize": {
      "type": "integer"
    },
    "totalSize": {
      "type": "integer",
      "required": true
    },
    "referringCompany": {
      "type": "string",
      "required": false
    },
    "client": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::client.client"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
